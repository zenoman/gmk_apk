<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="project.com.tastore">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_two"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_two"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon">

        <activity android:name="project.com.tastore.fragment.Cari_Barang" />
        <activity android:name="project.com.tastore.fragment.Detail_Barang" />
        <activity android:name="project.com.tastore.fragment.Detail_kategori" />
        <activity android:name="project.com.tastore.User.Bantuan" />
        <activity android:name="project.com.tastore.User.Register" />
        <activity android:name="project.com.tastore.User.Login" android:noHistory="true" />
        <activity android:name="project.com.tastore.MainActivity" />
        <activity android:name="project.com.tastore.welcome" android:noHistory="true" android:theme="@style/AppTheme.branded">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>